{
    "builds": [{
            "src": "project/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15md" , "runtime" : "python3.10.11"}
        }],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "project/wsgi.py"
            }
        ]
}